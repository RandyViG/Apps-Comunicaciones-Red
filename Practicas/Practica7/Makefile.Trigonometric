
# This is a template Makefile generated by rpcgen

# Parameters

CLIENT = Trigonometric_client
SERVER = Trigonometric_server

SOURCES_CLNT.c = 
SOURCES_CLNT.h = 
SOURCES_SVC.c = 
SOURCES_SVC.h = 
SOURCES.x = Trigonometric.x

TARGETS_SVC.c = Trigonometric_svc.c Trigonometric_server.c    
TARGETS = Trigonometric.h   Trigonometric_clnt.c Trigonometric_svc.c Trigonometric_client.c Trigonometric_server.c

OBJECTS_SVC = $(SOURCES_SVC.c:%.c=%.o) $(TARGETS_SVC.c:%.c=%.o)
# Compiler flags 

CFLAGS += -g

#Archlinux -ltirpc
#Debian -lnsl
LDLIBS += -ltirpc

RPCGENFLAGS = 

# Targets 

all : $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_SVC) : $(SOURCES_SVC.c) $(SOURCES_SVC.h) $(TARGETS_SVC.c) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.c) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS) -lm

 clean:
	 $(RM) core $(TARGETS) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)

